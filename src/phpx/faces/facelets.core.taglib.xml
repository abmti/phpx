<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns:phpfaces="urn:php-faces">
    
    <namespace>http://php.net/faces/core</namespace>

	<tag>
        <tag-name>view</tag-name>
    </tag>

    <tag>
        <tag-name>facet</tag-name>
    </tag>
    
    <tag>
        <tag-name>selectItems</tag-name>
        <component>
            <component-type>php.faces.SelectItems</component-type>
        </component>
    </tag>
    
    <tag>
        <tag-name>ajax</tag-name>
        <component>
            <component-type>php.faces.Ajax</component-type>
        </component>
    </tag>
    
</facelet-taglib>