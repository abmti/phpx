<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns:phpfaces="urn:php-faces">
    
    <namespace>http://php.net/faces/facelets</namespace>

    <tag>
        <tag-name>composition</tag-name>
    </tag>
	
	<tag>
        <tag-name>define</tag-name>
    </tag>
	
	<tag>
        <tag-name>include</tag-name>
    </tag>
    
    <tag>
        <tag-name>insert</tag-name>
    </tag>
    
    <tag>
        <tag-name>decorate</tag-name>
    </tag>
    
    <tag>
        <tag-name>repeat</tag-name>
        <component>
            <component-type>php.faces.Repeat</component-type>
        </component>
    </tag>
    
    
</facelet-taglib>